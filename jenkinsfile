pipeline{
    agent any

    stages {

        stage('prepare Artifacts') {
            steps {
                sh '''
                zip -r frontend.zip *
            '''
            }

        }
        stage('upload Artifacts') {
            steps {
                sh '''
           curl -f -v -u admin:admin --upload-file frontend.zip http://10.1.2.210:8081/repository/frontend-todo/frontend.zip
        '''
            }
        }
    }
}
